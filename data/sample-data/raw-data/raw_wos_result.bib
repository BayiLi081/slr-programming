@inproceedings{ WOS:000292277300011,
Author = {Hordijk, Michaela and Baud, Isa},
Editor = {OttoZimmermann, K},
Title = {Inclusive Adaptation: Linking Participatory Learning and Knowledge
   Management to Urban Resilience},
Booktitle = {RESILIENT CITIES: CITIES AND ADAPTATION TO CLIMATE CHANGE - PROCEEDINGS
   OF THE GLOBAL FORUM 2010},
