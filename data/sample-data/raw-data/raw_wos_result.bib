@article{ WOS:000303561600006,
Author = {Henstra, Daniel},
Title = {Toward the Climate-Resilient City: Extreme Weather and Urban Climate
   Adaptation Policies in Two Canadian Provinces},
Journal = {JOURNAL OF COMPARATIVE POLICY ANALYSIS},
Year = {2012},
Volume = {14},
Number = {2, SI},
Pages = {175-194},
Abstract = {Extreme weather events, such as unusually high or low temperatures,
   severe winds and heavy precipitation, pose a threat to people and
   property in cities, and are expected to become more frequent and intense
   as a result of climate change. Managing this risk requires effective
   climate adaptation policies - strategic courses of action designed to
   strengthen urban resilience to climate-related stress. City governments
   have a key role to play in adaptation policy design, but they appear to
   face challenges in marshalling political commitment and technical
   capacity. This article examines elements of urban climate adaptation
   policy targeting extreme weather and analyzes the policy development
   process in two major Canadian cities, Toronto and Halifax.},
