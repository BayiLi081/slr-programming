   Koba, Koffi and Utzinger, Juerg and Tanner, Marcel},
Editor = {OttoZimmermann, K},
Title = {Ecohealth and Climate Change: Adaptation to Flooding Events in Riverside
   Secondary Cities, West Africa},
Booktitle = {RESILIENT CITIES: CITIES AND ADAPTATION TO CLIMATE CHANGE - PROCEEDINGS
   OF THE GLOBAL FORUM 2010},
Series = {Local Sustainability},
Year = {2011},
Volume = {1},
Pages = {55+},
