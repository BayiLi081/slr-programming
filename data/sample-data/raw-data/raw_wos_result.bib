@inproceedings{ WOS:000292277300047,
Author = {Mathey, Juliane and Roessler, Stefanie and Lehmann, Iris and Braeuer,
   Anne},
Editor = {OttoZimmermann, K},
Title = {Urban Green Spaces: Potentials and Constraints for Urban Adaptation to
   Climate Change},
Booktitle = {RESILIENT CITIES: CITIES AND ADAPTATION TO CLIMATE CHANGE - PROCEEDINGS
   OF THE GLOBAL FORUM 2010},
Series = {Local Sustainability},
